// Coding In "C Language"//


//the percentage calculator
#include <stdio.h>
#include<conio.h>
int main()
{
    int sub1=85;
    int sub2=95;
    int sub3=39;
    int sub4=55;
    int sub5=95;
    int total= 500;
    int sum = sub1+sub2+sub3+sub4+sub5;
     float percent = sum*100/total;
    printf("Result : %f",percent);
    
    return 0 ;
    
}

